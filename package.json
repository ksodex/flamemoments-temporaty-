{
    "scripts": {
        "start:prod": "npm run build && npm run start:serve",
        "start:dev": "concurrently \"npm run build:watch\" \"npm run start:serve\"",
        "start:serve": "nodemon ./build/source/index.js",
        "build": "tsc -p tsconfig.json",
        "build:watch": "tsc --watch",
        "prisma:dev": "prisma migrate dev --name developmograte"
    },
    "devDependencies": {
        "@types/node": "^22.8.6",
        "concurrently": "^9.0.1",
        "nodemon": "^3.1.7",
        "prisma": "^5.21.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3"
    },
    "engines": {
        "node": ">=20.18.0",
        "npm": ">=10.8.2"
    },
    "dependencies": {
        "@fastify/cookie": "^11.0.1",
        "@fastify/helmet": "^12.0.1",
        "@prisma/client": "^5.21.1",
        "@types/bcrypt": "^5.0.2",
        "bcrypt": "^5.1.1",
        "dotenv": "^16.4.5",
        "fastify": "^5.1.0"
    }
}
